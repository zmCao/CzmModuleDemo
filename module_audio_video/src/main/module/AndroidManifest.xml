<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.czm.module.module_audio_video">
    <uses-permission android:name="android.permission.CAMERA"/>
    <application
        android:name="debug.AudioAndVideoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="debug.LauncherActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.czm.module_audio_video.VideoLearnActivity" />
        <activity android:name="com.czm.module_audio_video.VideoStreamingActivity"/>
        <activity android:name="com.czm.module_audio_video.HIKVisionActivity"/>
    </application>

</manifest>